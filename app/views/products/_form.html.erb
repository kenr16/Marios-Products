<%= render "layouts/errors", :object => @product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :picture %>
  <%= f.text_field :picture %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit  %>
<% end %>
